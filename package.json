{
  "name": "itb-portfolio",
  "version": "1.0.0",
  "description": "Eindopdracht 11 - Persoonlijke portfolio website",
  "main": "index.js",
  "scripts": {
    "sass": "sass --no-source-map site/_sass/main.scss site/_static/css/main.css",
    "watch:eleventy": "npx eleventy --serve --port=8080",
    "watch:sass": "npm run sass -- --watch",
    "serve": "npm-run-all sass --parallel watch:*",
    "build": "env NODE_ENV=production npm run sass && npx eleventy --pathprefix 'itb-portfolio'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgm-nicolascnudde/itb-portfolio.git"
  },
  "keywords": [],
  "author": "Nicolas Cnudde",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pgm-nicolascnudde/itb-portfolio/issues"
  },
  "homepage": "https://github.com/pgm-nicolascnudde/itb-portfolio#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.13"
  }
}
